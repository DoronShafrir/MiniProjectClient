<Page x:Class="MiniProject.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MiniProject"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainPage">

    <Grid Background="Wheat" RenderTransformOrigin="0.506,0.289">
        
        <Grid.RowDefinitions>
            <RowDefinition Height="4*"/>
            <RowDefinition Height="11*"/>        

        </Grid.RowDefinitions>      

    
    <Button x:Name="TryBtn" Content="Refresh" Click="TryBtn_Click"  HorizontalAlignment="Left" Height="25" Margin="255,60,0,0" VerticalAlignment="Top" Width="115"/>
        <StackPanel Grid.Row="1" x:Name="lstViewLecturer">
            <TextBlock Text="{Binding lstViewLecturer}"  FontSize="20" />

    </StackPanel>
    </Grid>
</Page>
